#include <stdio.h>

int main() {

    int a = 0;
  
  do
  {
      printf("YÃ¼kseklik");
      scanf("%d",&a);
  }
  while (a < 0 || a > 9);


    for (int b = 1; b <= a; b++) {
        for (int c = 0; c < a - b; c++) {
            printf(" ");
           }
          for (int c = 0; c < b; c++) {
           printf("#");
          
            
        }
      printf("   ");
      for (int d = 0; d < b; d++) {
        printf("#");
      }
      
      printf("\n");
   }

  

}
